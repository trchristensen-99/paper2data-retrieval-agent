OPENAI_API_KEY=your_openai_api_key_here

# Optional: cost-optimized default model for all agents
P2D_DEFAULT_MODEL=gpt-5-mini

# Optional per-agent overrides
# P2D_MODEL_METADATA=gpt-5-mini
